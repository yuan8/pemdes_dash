{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "devdojo/chatter": "0.2.*",
        "dompdf/dompdf": "^1.0",
        "fideloper/proxy": "^4.0",
        "graham-campbell/markdown": "10.3",
        "intervention/gif": "dev-master",
        "kreait/laravel-firebase": "2.4.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "nahid/jsonq": "^6.0",
        "nexmo/laravel-notification": "^0.2.1",
        "pbmedia/laravel-ffmpeg": "4.*",
        "php-ffmpeg/php-ffmpeg": "^0.18.0",
        "phpoffice/phpspreadsheet": "^1.16",
        "pomirleanu/gif-create": "^1.3",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "~4.0",
        "realrashid/sweet-alert": "^3.2",
        "shetabit/visitor": "^3.1",
        "unisharp/laravel-ckeditor": "^4.7"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
